enter align-items-center\">\n          <div className=\"Inner\">\n            <Pizzas pizzas={pizzas} />\n            <Sides sides={sides} />\n            <Drinks drinks={drinks} />\n          </div>\n\n          <Link to=\"/order/review\" className=\"OrderButton\">Edit/Finalize</Link>\n\n          <h4 className=\"Total\">\n            Total: Â£\n            {subTotal}\n          </h4>\n\n        </article>\n      </>\n    ) : (\n      <article id=\"Basket\" className=\"col-12 col-xl-2 h-100 d-flex justify-content-center align-items-center\">\n        <h4>No items in the basket...</h4>\n      </article>\n    )\n  );\n};\n\nexport default Basket;\n","/Users/alicandemirtas/dev/portfolio/new/pizza-store/frontend/src/Components/Order/Basket/utils.js",[],"/Users/alicandemirtas/dev/portfolio/new/pizza-store/frontend/src/Components/Order/Basket/_Pizzas.jsx",[],"/Users/alicandemirtas/dev/portfolio/new/pizza-store/frontend/src/Components/Order/Basket/_Drinks.jsx",[],"/Users/alicandemirtas/dev/portfolio/new/pizza-store/frontend/src/Components/Order/_ChooseCategory.jsx",[],"/Users/alicandemirtas/dev/portfolio/new/pizza-store/frontend/src/Components/Order/_ChoosePizzaSize.jsx",[],"/Users/alicandemirtas/dev/portfolio/new/pizza-store/frontend/src/Components/Order/Context/OrderStore.jsx",[],"/Users/alicandemirtas/dev/portfolio/new/pizza-store/frontend/src/Components/Order/Context/OrderReducer.js",[],"/Users/alicandemirtas/dev/portfolio/new/pizza-store/frontend/src/Components/Order/_ChoosePizzaToppings.jsx",["113"],"import React, { useContext, useEffect, useState } from 'react';\nimport { Link, useParams, Redirect } from 'react-router-dom';\nimport Basket from './Basket/Basket';\nimport { arrayEmpty } from './Basket/utils';\nimport { getToppings } from '../../api';\nimport { OrderContext } from './Context/OrderStore';\n\nconst ChoosePizzaToppings = () => {\n  const { size } = useParams();\n  const [availableToppings, setAvailableToppings] = useState([]);\n  const [toppings, setToppings] = useState([]);\n  const [submitted, setSubmitted] = useState(false);\n  const [, dispatch] = useContext(OrderContext);\n\n  useEffect(() => {\n    getToppings()\n      .then((ts) => {\n        setAvailableToppings(ts.map((t) => t.name));\n      });\n  }, []);\n\n  const pizzaPriceLookup = {\n    // eslint-disable-next-line quote-props\n    'small': 8.99, 'medium': 10.99, 'large': 12.99,\n  };\n\n  const submitPizza = () => {\n    const price = ((toppings.length) * 0.35) + pizzaPriceLookup[size];\n    dispatch({ type: 'ADD_PIZZA', payload: { toppings, size, price } });\n    setSubmitted(true);\n  };\n\n  const viewTopping = (topping) => (\n    <>\n      {(!arrayEmpty(toppings) && toppings.includes(topping)) && (\n        <button type=\"button\" className=\"Selected\" onClick={() => setToppings([...toppings, topping])}>{topping}</button>\n      )}\n      {((arrayEmpty(toppings) || !toppings.includes(topping))\n        && toppings.length < 6)\n        && (\n          <button type=\"button\" className=\"Available\" onClick={() => setToppings([...toppings, topping])}>{topping}</button>\n        )}\n      {!toppings.includes(topping) && toppings.length >= 6 && (\n        <button type=\"button\" onClick={() => console.log('TODO: #MaxToppingsModal')} data-toggle=\"modal\">{topping}</button>\n      )}\n    </>\n  );\n\n  if (submitted) {\n    return <Redirect to=\"/order\" />;\n  }\n\n  return (\n    <section id=\"PizzaTopping\">\n      <div id=\"PageContainer\" className=\"container-fluid\">\n        <div id=\"PageRow\" className=\"row\">\n\n          <Basket />\n\n          <div id=\"ExceptBasket\" className=\"col-xl-8 offset-xl-1\">\n            <article className=\"PageHead col-xl-12\">\n              <Link className=\"BackButton\" to=\"/order/pizza\">BACK</Link>\n              <h2 className=\"Header\">SELECT TOPPINGS</h2>\n            </article>\n\n            <div className=\"col-xl-12\">\n              <article id=\"Toppings\" className=\"row\">\n                {!arrayEmpty(availableToppings) && availableToppings.map((t) => (\n                  <div key={t} className=\"col-md-3 col-6\">\n                    {viewTopping(t)}\n                  </div>\n                ))}\n\n                {/* TODO: IMPLEMENT MODAL */}\n\n              </article>\n\n            </div>\n\n            <div className=\"col-12 d-flex justify-content-center\">\n              <button type=\"submit\" className=\"Done\" onClick={submitPizza}>DONE</button>\n            </div>\n          </div>\n\n        </div>\n      </div>\n    </section>\n  );\n};\n\nexport default ChoosePizzaToppings;\n","/Users/alicandemirtas/dev/portfolio/new/pizza-store/frontend/src/Components/Order/_ChooseSides.jsx",["114"],"import React, { useState, useEffect, useContext } from 'react';\nimport { Redirect } from 'react-router-dom';\n\nimport { OrderContext } from './Context/OrderStore';\nimport { arrayEmpty } from './Basket/utils';\nimport { getSides } from '../../api';\n\nimport Basket from './Basket/Basket';\n\nconst ChooseSides = () => {\n  const [availableSides, setAvailableSides] = useState([]);\n  const [done, setDone] = useState(false);\n  const [order, dispatch] = useContext(OrderContext);\n\n  useEffect(() => (\n    getSides()\n      .then((serverSides) => {\n        const clientSides = serverSides.map((s) => ({\n          name: s.name,\n          price: s.price,\n          quantity: 1,\n        }));\n        setAvailableSides(clientSides);\n      })\n  ), []);\n\n  const addSide = (side) => {\n    console.log('ADD SIDE');\n    return arrayEmpty(order.sides.filter((s) => s.name === side.name)) ? (\n      dispatch({\n        type: 'ADD_NEW_SIDE',\n        payload: side,\n      })\n    ) : (\n      dispatch({\n        type: 'INCREASE_SIDE_QUANTITY',\n        payload: side.name,\n      })\n    );\n  };\n\n  if (done) {\n    return <Redirect to=\"/order\" />;\n  }\n\n  return (\n    <section id=\"AddSide\">\n      <div id=\"PageContainer\" className=\"container-fluid\">\n        <div id=\"PageRow\" className=\"row\">\n\n          <Basket />\n\n          <div id=\"ExceptBasket\" className=\"col-xl-8 offset-xl-1\">\n            <article className=\"PageHead col-xl-12\">\n              <h2 className=\"Header\">ADD SIDES</h2>\n            </article>\n\n            <article id=\"Sides\" className=\"col-xl-12\">\n              <div className=\"row\">\n                {!arrayEmpty(availableSides) && availableSides.map((side) => (\n                  <div key={side.name} className=\"col-4\">\n                    <button\n                      onClick={() => addSide(side)}\n                      type=\"button\"\n                    >\n                      {side.name}\n                    </button>\n                  </div>\n                ))}\n              </div>\n            </article>\n\n            <div className=\"col-12 d-flex justify-content-center\">\n              <button\n                onClick={() => setDone(true)}\n                className=\"Done\"\n                type=\"submit\"\n              >\n                DONE\n              </button>\n            </div>\n          </div>\n        </div>\n      </div>\n    </section>\n  );\n};\n\nexport default ChooseSides;\n","/Users/alicandemirtas/dev/portfolio/new/pizza-store/frontend/src/Components/Order/Basket/_Sides.jsx",[],"/Users/alicandemirtas/dev/portfolio/new/pizza-store/frontend/src/Components/Order/_ChooseDrinks.jsx",["115"],{"ruleId":"116","replacedBy":"117"},{"ruleId":"118","replacedBy":"119"},{"ruleId":"120","replacedBy":"121"},{"ruleId":"122","replacedBy":"123"},{"ruleId":"124","replacedBy":"125"},{"ruleId":"126","replacedBy":"127"},{"ruleId":"128","severity":1,"message":"129","line":14,"column":3,"nodeType":"130","messageId":"131","endLine":14,"endColumn":14},{"ruleId":"128","severity":1,"message":"129","line":44,"column":46,"nodeType":"130","messageId":"131","endLine":44,"endColumn":57},{"ruleId":"128","severity":1,"message":"129","line":28,"column":5,"nodeType