p = {\n    // eslint-disable-next-line quote-props\n    'small': 8.99, 'medium': 10.99, 'large': 12.99,\n  };\n\n  const submitPizza = () => {\n    const price = (toppings.length * 0.35) + pizzaPriceLookup[size];\n    dispatch({\n      type: 'ADD_PIZZA',\n      payload: { toppings, size, price },\n    });\n    setSubmitted(true);\n  };\n\n  const toggleToppingSelected = (topping) => (\n    toppings.includes(topping) ? (\n      setToppings(\n        toppings.filter((t) => t !== topping),\n      )\n    ) : (\n      setToppings([...toppings, topping])\n    )\n  );\n\n  const viewTopping = (topping) => (\n    <>\n      {(!arrayEmpty(toppings) && toppings.includes(topping)) && (\n        <button\n          onClick={() => toggleToppingSelected(topping)}\n          className=\"Selected\"\n          type=\"button\"\n        >\n          {topping}\n        </button>\n      )}\n      {((arrayEmpty(toppings) || !toppings.includes(topping))\n        && toppings.length < 6)\n        && (\n          <button\n            onClick={() => toggleToppingSelected(topping)}\n            className=\"Available\"\n            type=\"button\"\n          >\n            {topping}\n          </button>\n        )}\n      {!toppings.includes(topping) && toppings.length >= 6 && (\n        <button\n          onClick={() => console.log('TODO: #MaxToppingsModal')}\n          data-toggle=\"modal\"\n          type=\"button\"\n        >\n          {topping}\n        </button>\n      )}\n    </>\n  );\n\n  if (submitted) {\n    return <Redirect to=\"/order\" />;\n  }\n\n  return (\n    <section id=\"PizzaTopping\">\n      <div id=\"PageContainer\" className=\"container-fluid\">\n        <div id=\"PageRow\" className=\"row\">\n\n          <Basket />\n\n          <div id=\"ExceptBasket\" className=\"col-xl-8 offset-xl-1\">\n            <article className=\"PageHead col-xl-12\">\n              <Link to=\"/order/pizza\" className=\"BackButton\">BACK</Link>\n              <h2 className=\"Header\">SELECT TOPPINGS</h2>\n            </article>\n\n            <div className=\"col-xl-12\">\n              <article id=\"Toppings\" className=\"row\">\n                {!arrayEmpty(availableToppings) && availableToppings.map((t) => (\n                  <div key={t} className=\"col-md-3 col-6\">\n                    {viewTopping(t)}\n                  </div>\n                ))}\n\n                {/* TODO: IMPLEMENT MODAL */}\n\n              </article>\n\n            </div>\n\n            <div className=\"col-12 d-flex justify-content-center\">\n              <button\n                onClick={submitPizza}\n                className=\"Done\"\n                type=\"submit\"\n              >\n                DONE\n              </button>\n            </div>\n\n          </div>\n        </div>\n      </div>\n    </section>\n  );\n};\n\nexport default ChoosePizzaToppings;\n","/Users/alicandemirtas/dev/portfolio/new/pizza-store/frontend/src/Components/Order/_ChooseSides.jsx",["132"],"import React, { useState, useEffect, useContext } from 'react';\nimport { Redirect } from 'react-router-dom';\n\nimport { OrderContext } from './Context/OrderStore';\nimport { arrayEmpty } from './Basket/utils';\nimport { getSides } from '../../api';\n\nimport Basket from './Basket/Basket';\n\nconst ChooseSides = () => {\n  const [availableSides, setAvailableSides] = useState([]);\n  const [done, setDone] = useState(false);\n  const [order, dispatch] = useContext(OrderContext);\n\n  useEffect(() => (\n    getSides()\n      .then((serverSides) => {\n        const clientSides = serverSides.map((s) => ({\n          name: s.name,\n          price: s.price,\n          quantity: 1,\n        }));\n        setAvailableSides(clientSides);\n      })\n  ), []);\n\n  const addSide = (side) => {\n    console.log('ADD SIDE');\n    return arrayEmpty(order.sides.filter((s) => s.name === side.name)) ? (\n      dispatch({\n        type: 'ADD_NEW_SIDE',\n        payload: side,\n      })\n    ) : (\n      dispatch({\n        type: 'INCREASE_SIDE_QUANTITY',\n        payload: side.name,\n      })\n    );\n  };\n\n  if (done) {\n    return <Redirect to=\"/order\" />;\n  }\n\n  return (\n    <section id=\"AddSide\">\n      <div id=\"PageContainer\" className=\"container-fluid\">\n        <div id=\"PageRow\" className=\"row\">\n\n          <Basket />\n\n          <div id=\"ExceptBasket\" className=\"col-xl-8 offset-xl-1\">\n            <article className=\"PageHead col-xl-12\">\n              <h2 className=\"Header\">ADD SIDES</h2>\n            </article>\n\n            <article id=\"Sides\" className=\"col-xl-12\">\n              <div className=\"row\">\n                {!arrayEmpty(availableSides) && availableSides.map((side) => (\n                  <div key={side.name} className=\"col-4\">\n                    <button\n                      onClick={() => addSide(side)}\n                      type=\"button\"\n                    >\n                      {side.name}\n                    </button>\n                  </div>\n                ))}\n              </div>\n            </article>\n\n            <div className=\"col-12 d-flex justify-content-center\">\n              <button\n                onClick={() => setDone(true)}\n                className=\"Done\"\n                type=\"submit\"\n              >\n                DONE\n              </button>\n            </div>\n          </div>\n        </div>\n      </div>\n    </section>\n  );\n};\n\nexport default ChooseSides;\n","/Users/alicandemirtas/dev/portfolio/new/pizza-store/frontend/src/Components/Order/Basket/_Sides.jsx",[],"/Users/alicandemirtas/dev/portfolio/new/pizza-store/frontend/src/Components/Order/_ChooseDrinks.jsx",["133","134"],"/Users/alicandemirtas/dev/portfolio/new/pizza-store/frontend/src/Components/Order/ReviewOrder.jsx",[],"/Users/alicandemirtas/dev/portfolio/new/pizza-store/frontend/src/Components/Order/utils.js",[],"/Users/alicandemirtas/dev/portfolio/new/pizza-store/frontend/src/Components/Order/_ReviewPizz